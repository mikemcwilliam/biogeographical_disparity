reps <- 5#
#
snn <- function(){#
	samp <- data[sample(nrow(data), size=n, replace=F),list]#
	dist <- dist(scale(samp))#
	knn <- get.knn(dist,k=neighbs)$nn.dist#
	rowSums(knn)#
	}#
